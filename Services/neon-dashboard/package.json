{
  "name": "neon-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
    "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.0"
  },
    "scripts": {
        "dev": "npm run watch",
        "build": "concurrently \"npm:build-*\"",
        "build-tailwind": "postcss --verbose ./Styles/index.css -o ./wwwroot/css/index.css",
        "build-isolated": "postcss --verbose ./obj/Debug/net7.0/scopedcss/bundle/neon-dashboard.styles.css -o ./wwwroot/css/isolated.css",
        "watch": "concurrently --kill-others \"npm:watch-*\"",
        "watch-tailwind": "postcss --verbose ./Styles/index.css -o ./wwwroot/css/index.css --watch",
        "watch-isolated": "postcss --verbose ./obj/Debug/net7.0/scopedcss/bundle/neon-dashboard.styles.css -o ./wwwroot/css/isolated.css --watch"
    },
